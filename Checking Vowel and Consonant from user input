#include <stdio.h>

int main()
{
    char ch;
    printf("Enter a character\n");
    scanf("%c",&ch);
    
    if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' || 
       ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U')
       {
           printf("Character is Vowel\n");
       }
    else 
        {
            printf("Character is Consonant\n");
        }
       
     
    return 0;
}
